{
    "minimum-stability": "dev",
    "name": "horde/translation",
    "description": "Translation library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Translation",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "time": "2021-11-24",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "ext-gettext": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Translation": "lib/"
        },
        "psr-4": {
            "Horde\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Translation\\Test\\": "test/"
        }
    }
}