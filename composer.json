{
    "name": "horde/translation",
    "description": "Translation library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Translation",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-08-19",
    "repositories": [],
    "require": {
        "php": "^7.4 || ^8"
    },
    "require-dev": {
        "horde/test": "^3"
    },
    "suggest": {
        "ext-gettext": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Translation": "lib/"
        },
        "psr-4": {
            "Horde\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Translation\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}